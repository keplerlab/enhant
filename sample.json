## Deleting a meeting
{
    "context": {
        "conv_id": "",
        "origin": "host / guest",
        "meeting_id": "",
        "event_time": ""
    },
    "msg": {
        "name": "DELETE",
        "type": "MEETING",
        "desc": "Delete record",
        "data": {
            "conversation":{ 
                "conv_id": "inserted_record_id_to_be_deleted"
            }
        },
        "version": ""
    },
    "response": [
        {
            "name": "DELETE",
            "type": "MEETING",
            "status": true,
            "id": "record_id",
        }
    ]
}




##  Transcription events

{
    "context": {
        "conv_id": "",
        "origin": "host / guest",
        "meeting_id": "",
        "event_time": ""
    },
    "msg": {
        "name": "ADD",
        "type": "TRANSCRIPTION",
        "desc": "ADD record",
        "data": {
            "transcription":{
                "content": "Transcription here..",
                "type": "text",
                "start_time":"",
                "end_time":""

            }
        },
        "version": ""
    },
    "response": [
        {
            "name": "ADD",
            "type": "TRANSCRIPTION",
            "record_id": "inserted_record_id",
            "status": {
                "code": "SUCCESS/FAILURE",
                "message": "msg"
            }
        }
    ]
}


## Notes event
{
    "context": {
        "conv_id": "",
        "origin": "host / guest",
        "meeting_id": "",
        "event_time": ""
    },
    "msg": {
        "name" : "ADD",
        "type" : "NOTE",
        "desc": "Update record",
        "data": {
            "note":{
                "content": "Note",
                "type": "text"
            }
        },
        "version": ""
    },
    "response": [
        {
            "name": "ADD",
            "type" : "NOTE",
            "status": true,
            "id": "record_id",
        }
    ]
}


## Example request for adding capture moment/bookmark
{
    "context": {
        "conv_id": "",
        "origin": "host / guest",
        "meeting_id": "",
        "event_time": ""
    },
    "msg": {
        "name": "ADD",
        "type" : "NOTE",
        "desc": "Add record",
        "data": {
            "note":{
                "content": "data:image/gif;base64,",
                "type": "image"
            }
        },
        "version": ""
    },
    "response": [
        {
            "name": "ADD",
            "type" : "NOTE",
            "id": "record_id",
            "status": true
        }
    ]
}

